{
    "name": "webard/nova-table-card",
    "description": "Nova table card. Show your latest data (orders, posts,...) as card or data you prefer.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0",
        "nova-kit/nova-packages-tool": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Whitespacecode\\TableCard\\": "src/"
        }
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Whitespacecode\\TableCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}